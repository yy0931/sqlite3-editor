{
  "name": "sqlite3-editor",
  "version": "1.0.0",
  "description": "",
  "main": "extension.js",
  "scripts": {
    "build": "./build.sh",
    "vscode:prepublish": "npm run build"
  },
  "engines": {
    "vscode": "1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:sqlite3-editor.editor"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "sqlite3-editor.pythonPath": {
          "type": "string",
          "default": ""
        },
        "sqlite3-editor.alwaysUsePyodide": {
          "type": "boolean",
          "default": false
        }
      }
    },
    "customEditors": [
      {
        "viewType": "sqlite3-editor.editor",
        "displayName": "SQLite3 Editor",
        "selector": [
          {
            "filenamePattern": "*.db"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "",
  "capabilities": {
    "virtualWorkspaces": false,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/vscode": "^1.71.0",
    "@types/which": "^2.0.1",
    "esbuild": "^0.15.7"
  },
  "dependencies": {
    "pyodide": "^0.21.3",
    "tempy": "^3.0.0",
    "which": "^2.0.2"
  }
}
